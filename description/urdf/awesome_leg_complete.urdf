    <link name="LF_FOOT">
        <collision>
            <origin xyz="0 0 0.02325" />
            <geometry>
                <sphere radius="0.031" />
            </geometry>
        </collision>
        
        <inertial>
            <mass value="0.1" />
            <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
        </inertial>

